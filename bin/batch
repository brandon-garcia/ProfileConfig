#!/bin/bash

PROG="$1"
shift

if [[ ! -z $(readlink -e "$PROG" 2>/dev/null) ]]; then
	PROG=$(readlink -e "$PROG" 2>/dev/null)
	if [[ "$#" -eq "1" ]]; then
		while read -r FILE || [[ -n "$f" ]]; do
			$PROG $FILE
		done<"$1"
	else
		while read -r FILE || [[ -n "$f" ]]; do
			$PROG $FILE
		done
	fi
else
	if [[ "$#" -eq "1" ]]; then
		while read -r FILE || [[ -n "$f" ]]; do
			eval "$PROG"
		done<"$1"
	else
		while read -r FILE || [[ -n "$f" ]]; do
			eval "$PROG"
		done
	fi
fi
